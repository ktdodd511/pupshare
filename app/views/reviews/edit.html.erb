<div class="container">
  <div class="review_form">


<h1>Edit Review </h1>
<%= form_for [@dog, @review] do |f| %>
<%= f.text_field :title, placeholder: "Title", class: "form-control" %></br>
<%= f.text_field :comment, placeholder: "Review", class: "form-control" %></br>

<%= f.submit "Update Review", class: "btn btn-primary" %>
<% end %>
  </div>
</div>
